{
  "editor.fontFamily": "Consolas, 'Microsoft YaHei', monospace",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.fontSize": 14,

  "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Microsoft YaHei', 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",

  "[javascript]": {
      "editor.formatOnSave": true
  },
  "prettier.singleQuote": true,

  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  
  "gitlens.advanced.messages": {
      "suppressShowKeyBindingsNotice": true
  },

  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": 0,
  "liveServer.settings.donotVerifyTags": true,
  "csharp.format.enable": false,
  "terminal.integrated.rendererType": "dom",
  "githubPullRequests.hosts": [
      {
          "host": "https://github.com",
          "username": "oauth",
          "token": "system"
      }
  ]
}